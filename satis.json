{
    "name": "lmf/composer-satis",
    "homepage": "https://allen-adventures.github.io/testcomposer-satis",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Allen-adVentures/OcrTools.git"
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ],
    "require": {
        "lmf/ocr-tools": "v1.0.3",
        "monolog/monolog": "^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/http-foundation": "^6.0",
        "laravel/framework": "^10.0",
        "doctrine/orm": "^2.0",
        "twig/twig": "^3.0",
        "phpunit/phpunit": "^10.0",
        "vlucas/phpdotenv": "^5.0",
        "ramsey/uuid": "^4.0",
        "league/flysystem": "^3.0",
        "psr/log": "^3.0",
        "psr/cache": "^3.0",
        "psr/container": "^2.0",
        "psr/http-message": "^1.0",
        "psr/simple-cache": "^3.0"
    },
    "require-all": false,
    "require-dependencies": true,
    "require-dev-dependencies": true,
    "config": {
        "secure-http": true
    },
    "output-dir": "public",
    "output-html": true,
    "allow-seo-indexing": true,
    "archive": {
        "directory": "dist",
        "format": "tar",
        "prefix-url": "https://allen-adventures.github.io/testcomposer-satis/dist",
        "skip-dev": false
    }
}
