{
    "name": "lmf/composer-satis",
    "homepage": "https://allen-adventures.github.io/testcomposer-satis",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Allen-adVentures/OcrTools.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/symfony/symfony.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laravel/framework.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/monolog/monolog.git"
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ],
    "require": {
        "lmf/ocr-tools": "v1.0.3"
    },
    "require-all": false,
    "require-dependencies": false,
    "require-dev-dependencies": false,
    "config": {
        "secure-http": true
    },
    "output-dir": "public",
    "output-html": true,
    "allow-seo-indexing": true,
    "archive": {
        "directory": "dist",
        "format": "tar",
        "prefix-url": "https://allen-adventures.github.io/testcomposer-satis/dist",
        "skip-dev": false
    }
}
